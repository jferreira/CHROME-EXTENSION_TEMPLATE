{
  "name": "Name: Template Chrome Extension",
  "version": "1.0",
  "description": "It is a template file for a chrome extension!",
  "manifest_version": 2,
  "background": {
    "scripts": ["scripts/background.js"]
  },
    "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
    "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
